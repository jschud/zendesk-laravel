{
    "name": "huddledigital/zendesk-laravel",
    "type": "library",
    "description": "Laravel wrapper for zendesk/zendesk_api_client_php package",
    "keywords": ["zendesk"],
    "homepage": "http://workwithhuddle.com",
    "authors": [
        {
            "name": "Huddle Digital Ltd",
            "email": "info@workwithhuddle.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/jschud/zendesk_api_client_php"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "~5.4",
        "zendesk/zendesk_api_client_php": "dev-auth"
    },
    "autoload": {
        "psr-4": {
            "Huddle\\Zendesk\\": "src"
        }
    },
    "minimum-stability": "stable"
}
